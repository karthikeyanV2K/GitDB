<service>
	<id>gitdb.exe</id>
	<name>GitDB</name>
	<description>GitDB - GitHub-backed NoSQL Database Server</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>X:\git Database\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>X:\git Database\dist\main.js</argument>
	<argument>--scriptoptions=server</argument>
	<argument>--log</argument>
	<argument>GitDB wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<env name="NODE_ENV" value="production"/>
	<serviceaccount>
		<domain>MSI</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>X:\git Database</workingdirectory>
</service>