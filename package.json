{
  "name": "gitdb-database",
  "version": "2.0.13",
  "description": "GitHub-backed NoSQL Database with CLI, Shell, and REST API - Store your data in GitHub repositories",
  "main": "dist/main.js",
  "type": "commonjs",
  "bin": {
    "gitdb": "./bin/gitdb.js",
    "gitdb-shell": "./bin/gitdb-shell.js",
    "gitdb-server": "./bin/gitdb-server.js"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "node scripts/build-production.js",
    "build:all": "node scripts/build-all.js",
    "build:executables": "npm run build:unified",
    "build:unified": "npx pkg dist/main.js --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --output dist/executables/gitdb",
    "build:installers": "npm run build:prod && npm run create:installers",
    "build:msi": "npm run build:prod && powershell -ExecutionPolicy Bypass -File installers/create-msi-installers.ps1",
    "build:msi:x64": "npm run build:prod && powershell -ExecutionPolicy Bypass -File installers/create-msi-installers.ps1 -Architecture x64",
    "build:msi:x86": "npm run build:prod && powershell -ExecutionPolicy Bypass -File installers/create-msi-installers.ps1 -Architecture x86",
    "create:installers": "node scripts/create-installers.js",
    "install:windows": "installers\\install-gitdb-windows.bat",
    "install:macos": "chmod +x installers/install-gitdb-macos.sh && installers/install-gitdb-macos.sh",
    "install:linux": "chmod +x installers/install-gitdb-linux.sh && installers/install-gitdb-linux.sh",
    "start": "node dist/main.js server",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "shell": "node dist/main.js shell",
    "cli": "node dist/main.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "publish:npm": "npm publish",
    "publish:npm:beta": "npm publish --tag beta"
  },
  "keywords": [
    "nosql",
    "database",
    "github",
    "cli",
    "shell",
    "git",
    "storage",
    "json",
    "production",
    "api",
    "global",
    "service"
  ],
  "author": "GitDB Team",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "21.0.2",
    "chalk": "^5.4.1",
    "commander": "11.1.0",
    "express": "4.18.2",
    "node-windows": "1.0.0-beta.8",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.10",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karthikeyanV2K/gitdb.git"
  },
  "bugs": {
    "url": "https://github.com/karthikeyanV2K/gitdb/issues"
  },
  "homepage": "https://github.com/karthikeyanV2K/gitdb#readme",
  "os": [
    "win32",
    "linux",
    "darwin"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "dist/**/*.js",
      "dist/**/*.json",
      "node_modules/node-windows/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist/executables"
  },
  "files": [
    "dist/",
    "bin/",
    "README.md",
    "LICENSE"
  ]
}
